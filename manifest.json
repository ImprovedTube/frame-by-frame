{
    "manifest_version": 2,
    "name": "Video Frame By Frame ( slowmo gif facebook )",
    "version": "0.12",
    "version_name": "0.12 alpha",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "description": "Video frame by frame. Move the mouse over videos + use arrow-keys to play frames. Slow motion reddit 9gag twitter tiktok animation",
    
    "content_scripts": [{
        "all_frames": true,
        "js": [
            "content-scripts.js"
        ],
        "css": [
            "content-scripts.css"
        ],
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "run_at": "document_start"
    }]
}
